{
  "name": "moonlar-server",
  "version": "1.0.0",
  "main": "dist/Server.js",
  "author": "Miguel Ã‚ngelo <miguelcg958@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "16.6.0",
    "yarn": "1.22.5"
  },
  "scripts": {
    "start": "set NODE_ENV=production&&node ./dist/Server.js",
    "dev": "set NODE_ENV=development&&ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/Server.ts",
    "test": "set NODE_ENV=test&&jest -i --forceExit --detectOpenHandles",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "knex:seed": "set NODE_ENV=development&&knex --knexfile knexfile.ts seed:run",
    "knex:migrate": "set NODE_ENV=development&&knex --knexfile knexfile.ts migrate:latest"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jest-json-schema": "^2.1.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "jest": "^27.0.6",
    "jest-json-schema": "^5.0.0",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
